function _generate_config() {
	local target_variable_name;
	import_args "$@";
	check_required_arguments "_generate_config" target_variable_name;
	local vpc_name="$container.getAttribute("aws_vpc_name")";
	local bastion_name="$instance.getAttribute("bastion_name")";
	local bastion_private_key_file="$instance.getAttribute("private_key_file")";
	local ssh_username="$instance.getAttribute("ssh_username")";
	local aws_region="$instance.getAttribute("aws_region")";
	
#[[	

	get_bastion_ssh_config --vpc_name "$vpc_name" \
	    --region "$aws_region" \
		--bastion_name "$bastion_name" \
		--bastion_private_key_file "$bastion_private_key_file" \
		--ssh_username "$ssh_username" \
		--target_variable_name "$target_variable_name"
		
}

_generate_config "$@";
]]#
